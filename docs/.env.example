# Database Configuration
DB_USERNAME=postgres
DB_PASSWORD=postgres

# LLM Studio Configuration (Local AI)
LLM_STUDIO_BASE_URL=http://localhost:1234/v1
LLM_STUDIO_MODEL=mistral-7b-instruct-v0.3
LLM_STUDIO_EMBEDDING_MODEL=nomic-embed-text
LLM_STUDIO_API_KEY=not-needed
LLM_STUDIO_TIMEOUT=120000

# Application Configuration
UPLOAD_DIR=./uploads

# Server Configuration
SERVER_PORT=8080

# Note: If you want to switch to OpenAI instead of local LLM:
# Uncomment and set your OpenAI API key
# OPENAI_API_KEY=sk-your-key-here
# Update application.yml to use ai.openai configuration instead of ai.llm-studio
